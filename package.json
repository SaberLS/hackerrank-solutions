{
  "name": "hackerrank-solutions",
  "version": "1.0.0",
  "description": "",
  "license": "ISC",
  "author": "SaberLS",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "lint:check": "eslint \"**/*.{ts,tsx,mts,cts,cjs,mjs,js,jsx,md}\"",
    "lint:fix": "eslint --fix \"**/*.{ts,tsx,mts,cts,cjs,mjs,js,jsx,md}\"",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{ts,mts,cts,tsx,js,mjs,cjs,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "^8.34.1",
    "@typescript-eslint/parser": "^8.34.1",
    "eslint": "^9.25.0",
    "eslint-plugin-unicorn": "^59.0.1",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "prettier": "^3.7.4",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-packagejson": "^2.5.15",
    "prettier-plugin-sort-json": "^4.1.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.34.1"
  }
}
